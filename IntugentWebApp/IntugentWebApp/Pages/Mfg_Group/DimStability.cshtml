@page
@model IntugentWebApp.Pages.Mfg_Group.DimensionStabilityModel
@{
    ViewData["ActivePage"] = "Dimension Stability";
}

@if (TempData["ErrorOnServer"] != null)
{
    <div class="alert alert-danger alert-dismissible fade show" role="alert">
        Error: @TempData["ErrorOnServer"]
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
}
<div class="row ">
    <!-- General Info Card -->
    <div class="card card-default col-md-3" >
        <div class="card-header align-items-center">
            <h2 class="">General Info</h2>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-lg-12 mb-2 d-flex align-items-center">
                    <!-- Dataset -->
                    <label class="col-3 col-form-label text-end">Dataset</label>
                    <div class="input-group col-9 d-flex">
                        <button class="btn btn-primary m-auto" type="button" style="padding: 9px" name="" onclick=""><span class="mdi mdi-chevron-left"></span></button>
                        <input type="number" class="form-control text-center w-50" disabled value="@Model.gID">
                        <button onclick="" class="btn btn-primary m-auto" type="button" style="padding: 9px"><span class="mdi mdi-chevron-right"></span></button>
                    </div>
                </div>



            </div>
            <div class="row">
                <div class="col-md-6">
                    <!-- mfgDate -->
                    <div class="form-floating mb-2">
                        <input type="datetime-local" class="form-control" id="mfgDate" value="@Model.gProductionDate" disabled>
                        <label for="mfgDate">Production Date</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- productCode -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="productCode" placeholder="" value="@Model.gProductCode" disabled>
                        <label for="productCode">Product Code</label>
                    </div>
                </div>
            </div>
            <div class="row">


                <div class="col-md-6">
                    <!-- MfgTimeStamp -->
                    <div class="form-floating mb-2">
                        <input type="datetime-local" class="form-control" id="Mfg Time Stamp" value="@Model.gProductionTime" onblur="" disabled>
                        <label for="Mfg Time Stamp">Mfg Time Stamp</label>
                    </div>
                </div>



                <div class="col-md-6">
                    <!-- DimTestingComplete -->
                    <div class="custom-control custom-checkbox align-items-center">
                        <input type="checkbox" class="custom-control-input" id="fgTestingComplete" checked="@Model.gDimStabilityDoneIsChecked" onchange="">
                        <label class="custom-control-label" for="fgTestingComplete">Dimensional Stability Testing Complete</label>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <!-- Dimesion Stability edge collapsed -->
    <div class="card card-default col-md-2 ">
        <div class="card-header text-left align-items-center">
            <h2 class="">Dimension Stability Edge Collapsed</h2>
        </div>
        <div class="card-body">
            <div class="row mb-2">
                <!-- DimTestingComplete -->
                <div class="custom-control custom-checkbox align-items-center">
                    <input type="checkbox" class="custom-control-input" id="Edge H1" checked="@Model.gEdgeH1IsChecked" onchange="">
                    <label class="custom-control-label" for="Edge H1">Edge H1</label>
                </div>
                </div>
            <div class="row mb-2">
                <!-- DimTestingComplete -->
                <div class="custom-control custom-checkbox align-items-center">
                    <input type="checkbox" class="custom-control-input" id="Edge H2" checked="@Model.gEdgeH2IsChecked" onchange="">
                    <label class="custom-control-label" for="Edge H2">Edge H2</label>
                </div>
            </div>
            <div class="row mb-2">
                <!-- DimTestingComplete -->
                <div class="custom-control custom-checkbox align-items-center">
                    <input type="checkbox" class="custom-control-input" id="Edge C1" checked="@Model.gEdgeC1IsChecked" onchange="">
                    <label class="custom-control-label" for="Edge C1">Edge C1</label>
                </div>
            </div>
            <div class="row mb-2">
                <!-- DimTestingComplete -->
                    <div class="custom-control custom-checkbox align-items-center">
                    <input type="checkbox" class="custom-control-input" id="Edge C2" checked="@Model.gEdgeC2IsChecked" onchange="">
                    <label class="custom-control-label" for="Edge C2">Edge C2</label>
                    </div>
            </div>

        </div>
    </div>
    <!-- Dim Stability % Change in dims -->
    <div class="card card-default col-md-3">
        <div class="card-header align-items-center">
            <h2 class="">Dimensions Stability % Change in dimensions</h2>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-md-6">
                    <!-- % Change Length Oven -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Change Length Oven" placeholder="" value="@Model.gChangeOvenLength" disabled>
                        <label for="Change Length Oven">% Change Length Oven</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- % Change Width Oven -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="ChangeWidthOven" placeholder="" value="@Model.gChangeOvenWidth" disabled>
                        <label for="ChangeWidthOven">% Change Width Oven</label>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <!-- % Change Thickness Oven -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Change Thickness Oven" placeholder="" value="@Model.gChangeOvenThickness" disabled>
                        <label for="Change Thickness Oven">% Change Thickness Oven</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- % Change Length Freezer -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Change Length Freezer" placeholder="" value="@Model.gChangeFreezerLength" disabled>
                        <label for="Change Length Freezer">% Change Length Freezer</label>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <!-- % Change Width Freezer -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Change Width Freezer" placeholder="" value="@Model.gChangeFreezerWidth" disabled>
                        <label for="Change Width Freezer">% Change Width Freezer</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- % Change Thickness Freezer -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Change Thickness Freezer" placeholder="" value="@Model.gChangeFreezerThickness" disabled>
                        <label for="Change Thickness Freezer">% Change Thickness Freezer</label>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Walk-in Freezer: Test Summary -->
    <div class="card card-default col-md-2">
        <div class="card-header align-items-center">
            <h2 class="">Walk-in Freezer: Test Summary</h2>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-md-12">
                    <!-- % Change Length -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Change Length" placeholder="" value="@Model.gChangeWFLength" disabled>
                        <label for="Change Length">% Change Length</label>
                    </div>
                </div>
            </div>
            <div class="row">
            <div class="col-md-12">
                    <!-- % Change Width -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="Change Width" placeholder="" value="@Model.gChangeWFWidth" disabled>
                        <label for="Change Width">% Change Width</label>
                    </div>
                </div>
             </div>
            <div class="row">
                <div class="col-md-6">
                    <!-- Average Depth -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="AverageDepth" placeholder="" value="@Model.gAvgDepth" disabled>
                        <label for="AverageDepth">Average Depth</label>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- Max Depth -->
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="MaxDepth" placeholder="" value="@Model.gMaxDepth" disabled>
                        <label for="MaxDepth">Max Depth</label>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Walk-in Freezer Testing -->
    <div class="card card-default h-auto col-md-2">
        <div class="card-header align-items-center">
            <h2 class="">Walk-in Freezer Testing</h2>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-md-12">
                    <!-- Date & time sample put in Freezer -->
                    <div class="form-floating mb-2">
                        <input type="datetime-local" class="form-control" id="DatetimesampleputinFreezer" placeholder="" value="@Model.gTestDateTime">
                        <label for="DatetimesampleputinFreezer">Date & time sample put in Freezer</label>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <!-- Deviation from Table -->
                    <label for="DeviationfromTable">Deviation from Table</label>
                    <div class="form-floating mb-2">
                        <input type="text" class="form-control" id="DeviationfromTable" placeholder="" value="@Model.gDeviation">
                    </div>
                </div>

                <div class="col-md-6">
                    <!-- Deviation (Bowing) Type -->
                    <label for="DeviationType">Deviation Type</label>
                    <div class="form-floating mb-2">
                        <select class="form-select" id="DeviationType">
                            <option value="" selected="">Up</option>
                            <option value="" selected="">Down</option>
                        </select>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>

